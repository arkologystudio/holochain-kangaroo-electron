    "ncp": "^2.0.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@holochain/client": "^0.16.1",
    "@lightningrodlabs/electron-holochain": "^0.7.8",
    "@napi-rs/cli": "^2.16.3",
