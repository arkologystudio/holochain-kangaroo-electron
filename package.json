{
  "name": "data-commons-mvp",
  "productName": "data-commons-mvp",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "dist": "rimraf holochain-data && rimraf main_window && make-dir ./main_window && ncp ./electron/ ./main_window/ && cross-unzip ./pouch/ui.zip ./main_window && make-dir binaries && ncp node_modules/@lightningrodlabs/electron-holochain/binaries/ ./binaries/ && node ./scripts/inject-renderer-import.js",
    "start": "npm run dist && electron-forge start",
    "package": "npm run dist && electron-forge package",
    "make": "npm run dist && electron-forge make",
    "publish": "npm run dist && electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Matt Gabrenya",
    "url": "https://buildyourweb.app",
    "email": "matt@buildyourweb.app"
  },
  "license": "MIT",
